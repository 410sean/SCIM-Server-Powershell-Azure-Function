{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": { 
    "usersPath": {
      "matchCondition": {
          "route": "api/users/{*path}"
      },
      "backendUri": "https://localhost/api/usersEndpoint"
    },
    "users": {
      "matchCondition": {
          "route": "api/users"
      },
      "backendUri": "https://localhost/api/usersEndpoint"
    },
    "schemasPath": {
      "matchCondition": {
          "route": "api/schemas/{*path}"
      },
      "backendUri": "https://localhost/api/schemasEndpoint"
    },
    "schemas": {
      "matchCondition": {
          "route": "api/schemas"
      },
      "backendUri": "https://localhost/api/schemasEndpoint"
    },
    "ResourceTypesPath": {
      "matchCondition": {
          "route": "api/ResourceTypes/{*path}"
      },
      "backendUri": "https://localhost/api/ResourceTypesEndpoint"
    },
    "ResourceTypes": {
      "matchCondition": {
          "route": "api/ResourceTypes"
      },
      "backendUri": "https://localhost/api/ResourceTypesEndpoint"
    }
   }
}
